list = ["水", "金", "地", "火", "木", "土", "天", "海", "冥"]

# リストの中身を繰り返し取り出す
for i in range(1, 20):
    # リストの長さで割った余りをインデックスとして使用（0始まりに調整）
    print(f"{i}{list[(i-1) % len(list)]}")

list = ["水", "金", "地", "火", "木", "土", "天", "海", "冥"]

i = 1
while i <= 19:
    # リストの長さで割った余りをインデックスとして使用（0始まりに調整）
    print(f"{i}{list[(i-1) % len(list)]}")
    i += 1
