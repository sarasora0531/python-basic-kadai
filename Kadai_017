class Human:
    def __init__(self, name, age):
        self.name = name
        self.age = age

    def check_adult(self):
        if self.age >= 20:
            print(f"{self.name} is an adult.")
        else:
            print(f"{self.name} is not an adult.")

# Humanクラスのインスタンスを作成してリストに追加
humans = [
    Human("Alice", 22),
    Human("Bob", 17),
    Human("Charlie", 35),
    Human("Diana", 19)
]

# リストの要素数分だけcheck_adultメソッドを呼び出す
for human in humans:
    human.check_adult()
